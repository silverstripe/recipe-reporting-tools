{
    "name": "silverstripe/recipe-reporting-tools",
    "type": "silverstripe-recipe",
    "description": "Add extra CMS reporting tools to your SilverStripe project",
    "homepage": "https://www.silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.4 || ^8.0",
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "~4.12.0@stable",
        "silverstripe/externallinks": "~2.3.0@stable",
        "silverstripe/securityreport": "~2.5.0@stable",
        "silverstripe/sitewidecontent-report": "~3.3.0@stable",
        "bringyourownideas/silverstripe-maintenance": "~2.6.0@stable",
        "bringyourownideas/silverstripe-composer-update-checker": "~3.0.0@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symbiote/silverstripe-queuedjobs": "^4.9"
    },
    "extra": {
        "project-files": []
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}