{
    "name": "silverstripe/recipe-reporting-tools",
    "type": "silverstripe-recipe",
    "description": "Add extra CMS reporting tools to your SilverStripe project",
    "homepage": "https://www.silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "^4.2",
        "silverstripe/externallinks": "2.0.x-dev",
        "silverstripe/securityreport": "2.1.x-dev",
        "silverstripe/sitewidecontent-report": "3.0.x-dev",
        "bringyourownideas/silverstripe-maintenance": "2.1.x-dev",
        "bringyourownideas/silverstripe-composer-update-checker": "2.0.x-dev",
        "bringyourownideas/silverstripe-composer-security-checker": "2.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "project-files": []
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
