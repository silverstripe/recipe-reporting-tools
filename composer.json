{
    "name": "silverstripe/recipe-reporting-tools",
    "type": "silverstripe-recipe",
    "description": "Add extra CMS reporting tools to your SilverStripe project",
    "homepage": "https://www.silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.3 || ^8.0",
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "~4.10.0@stable",
        "silverstripe/externallinks": "~2.2.0@stable",
        "silverstripe/securityreport": "~2.4.0@stable",
        "silverstripe/sitewidecontent-report": "~3.2.0@stable",
        "bringyourownideas/silverstripe-maintenance": "~2.4.1@stable",
        "bringyourownideas/silverstripe-composer-update-checker": "~2.1.0@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "project-files": []
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}