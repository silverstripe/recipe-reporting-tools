{
    "name": "silverstripe/recipe-reporting-tools",
    "type": "silverstripe-recipe",
    "description": "Add extra CMS reporting tools to your SilverStripe project",
    "homepage": "https://www.silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.4 || ^8.0",
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "4.12.x-dev",
        "silverstripe/externallinks": "2.3.x-dev",
        "silverstripe/securityreport": "2.5.x-dev",
        "silverstripe/sitewidecontent-report": "3.3.x-dev",
        "bringyourownideas/silverstripe-maintenance": "2.6.x-dev",
        "bringyourownideas/silverstripe-composer-update-checker": "3.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symbiote/silverstripe-queuedjobs": "^4.9"
    },
    "extra": {
        "project-files": []
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
